{
	"name": "owldarker-theme",
	"version": "1.1.0",
	"displayName": "Owl Darker",
	"homepage": "https://github.com/gabrielmaialva33/vs-owldarker-theme",
	"description": "Official Owl Darker Theme. A dark theme for many editors, shells, and more.",
	"publisher": "OwlDarkerTheme",
	"license": "MIT",
	"scripts": {
		"build": "node ./scripts/build.js",
		"lint": "node ./scripts/lint.js",
		"package": "vsce package -o ./bin/owldarker.vsix",
		"vscode:prepublish": "npm run build",
		"vsce-publish": "vsce publish"
	},
	"maintainers": [
		"Gabriel Maia <gabrielmaialva33@gmail.com>"
	],
	"contributors": [
		"Gabriel Maia"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/gabrielmaialva33/vs-owldarker-theme.git"
	},
	"bugs": {
		"url": "https://github.com/gabrielmaialva33/vs-owldarker-theme/issues"
	},
	"engines": {
		"vscode": "^1.13.0"
	},
	"categories": [
		"Themes"
	],
	"keywords": [
		"owldarker",
		"dark",
		"theme",
		"color-theme"
	],
	"icon": "icon.png",
	"galleryBanner": {
		"color": "#15121E",
		"theme": "dark"
	},
	"contributes": {
		"themes": [
			{
				"label": "OwlDarker Theme With Italics",
				"uiTheme": "vs-dark",
				"path": "./themes/owldarker-theme-with-italics.json"
			},
			{
				"label": "OwlDarker Theme With Italics Less Dark",
				"uiTheme": "vs-dark",
				"path": "./themes/owldarker-theme-with-italics-less-dark.json"
			},
			{
				"label": "OwlDarker Theme With Italics Less Dark 2",
				"uiTheme": "vs-dark",
				"path": "./themes/owldarker-theme-with-italics-less-dark-2.json"
			},
			{
				"label": "OwlDarker Theme No Italics",
				"uiTheme": "vs-dark",
				"path": "./themes/owldarker-theme-no-italics.json"
			},
			{
				"label": "OwlDarker Theme No Italics Less Dark",
				"uiTheme": "vs-dark",
				"path": "./themes/owldarker-theme-no-italics-less-dark.json"
			},
			{
				"label": "OwlDarker Theme No Italics Less Dark 2",
				"uiTheme": "vs-dark",
				"path": "./themes/owldarker-theme-no-italics-less-dark-2.json"
			}
		]
	}
}
